    \uC9C8\uBB38\uC5D0\uC11C \uBC29\uD5A5\uC744 \uB098\uD0C0\uB0B4\uB294 \uD0A4\uC6CC\uB4DC(\"\uC55E\",
    \"\uC624\uB978\uCABD\", \"\uB4A4\", \"\uC67C\uCABD\" \uB4F1)\uB97C \uD30C\uC545\uD558\uACE0,
